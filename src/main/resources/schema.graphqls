type Query {
    UserById(id: String): User
}

type User{
    id: String,
    email: String,
    username: String,
    firstname: String,
    lastname: String
    preferences: [Preference]
    recommendations: [Recommendation]
}

type Rating{
    id: String
}

type Recommendation{
    id: String
}

type Preference{
    id: String
}

type Activity{
    id: String
}